{
  "name": "pet-health-backend",
  "version": "1.0.0",
  "description": "Backend API for Pet Health Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "build": "echo 'No build step required for Node.js'",
    "docker:build": "docker build -t pet-health-backend .",
    "docker:run": "docker run -p 5000:5000 pet-health-backend",
    "db:login": "npx supabase login",
    "db:link": "npx supabase link",
    "db:migrate": "npx supabase db push",
    "db:diff": "npx supabase db diff --linked --file 0001_init.sql",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset --linked --debug",
    "db:seed": "npx supabase db reset --seed",
    "db:status": "npx supabase status",
    "db:lint": "npx supabase db lint",
    "db:new-migration": "npx supabase migration new",
    "db:local": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:logs": "npx supabase logs",
    "db:studio": "npx supabase studio"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.7.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "pet",
    "health",
    "management",
    "api",
    "express",
    "supabase"
  ],
  "author": "Pet Health Team",
  "license": "MIT"
}
